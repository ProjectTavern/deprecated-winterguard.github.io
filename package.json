    "cross-env": "^7.0.2",
    "gray-matter": "^4.0.2",
    "js-yaml": "^3.14.0",
    "marked": "^2.1.3",
    "node-fetch": "^2.6.1",
    "object-path": "^0.11.5",
    "prettier": "^2.1.2",
    "push-dir": "^0.4.1",
