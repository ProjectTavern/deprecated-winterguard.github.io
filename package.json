{
  "name": "winterguard.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env next dev",
    "build": "cross-env next build",
    "build:static": "cross-env next build && next export",
    "deploy:gh-pages": "cross-env touch out/.nojekyll && push-dir --dir=out --branch=gh-pages --cleanup",
    "publish:gh-pages": "cross-env npm run build:static && npm run deploy:gh-pages",
    "start": "cross-env next start",
    "book:start": "cross-env start-storybook -p 10090"
  },
  "dependencies": {
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-backgrounds": "^6.1.20",
    "@storybook/addon-console": "^1.2.3",
